@model GradDodajVM



@{
    ViewData["Title"] = "View";
}

<h1>View</h1>

<form asp-action="SnimiGrad" asp-controller="Grad">


    <input asp-for="Id" style="display:none" />

    <div class="form-group">
        <label class="control-label">Naziv</label>
        <div>
            <input asp-for="Naziv" class="form-control" />
            <span asp-validation-for="Naziv" class="text-danger"></span>
        </div>

    </div>
    <div class="form-group">
        <label class="control-label">Postanski broj</label>
        <input asp-for="PostanskiBroj" class="form-control" type="text" />
        <span asp-validation-for="PostanskiBroj" class="text-danger"></span>

    </div>

    <div class="form-group">
        <label class="control-label">Zupanija</label>
        <select asp-for="ZupanijaId" asp-items="Model.zupanije" class="form-control"></select>
    </div>

    <input type="submit" value="Dodaj" class="btn btn-primary" />

</form>

<a asp-action="Index">Povratak</a>

